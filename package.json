    "zip": "run-s clean archive",
    "start": "run-s clean dev"
  },
  "devDependencies": {
    "netlify-cli": "^2.70.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.2"
  }
}
